html,body{
    background: rgb(247,247,247);
    font-family: "PingFangSC-Regular" !important;
    overflow-x:hidden !important;
    overflow-y:auto;
}
nav{
    background: rgb(250,250,250);
    height: 2.5rem;
    line-height: 2.5rem;
    text-align: center;
    display: flex;
    display: -webkit-flex;
}
nav .tohome,
nav .contact{
    flex: 1;
    -webkit-flex: 1;
    height: 1.8rem !important;
    line-height: 1.8rem;
    margin-top: .35rem;
}
nav .tohome{
    border-right: 1px solid #c6c6c6;
}
.bar-tab .tab-item .icon{
    top:-.05rem!important;
}

nav.bar .tohome .icon img,
nav.bar .contact .icon img{
    height: 1.2rem;
    width: 1.2rem;
}
nav.bar-tab .go_change{
    flex: 5;
    -webkit-flex: 5;
    background: rgb(222,63,55);
    font-size: .85rem;
    color: rgb(255,255,255);
}

.page .bind_phone{
    position: fixed;
    width: 90%;
    height: 8.7rem;
    left: 50%;
    top: 50%;
    margin-top: -40%;
    margin-left: -45%;
    border: 1px solid #c8c8c8;
    border-radius: 5px;
    background: rgb(255,255,255);
    z-index: 1001;
    display: none;
}
.page .bind_phone .bind_title{
    text-align: center;
    font-size: .8rem;
    height: 2.2rem;
    line-height: 2.2rem;
}
.page .bind_phone .list-block{
    margin: 0;
}
.page .bind_phone .list-block ul li .item-content{
    padding-left: 0.5rem;
}
.page .bind_phone .list-block ul li .item-content .item-inner{
    padding-right: .5rem;
}
.page .bind_phone .list-block ul li .item-content .item-inner .item-title.label{
    width: 20%;
    font-size: .75rem;
}
.page .bind_phone .list-block ul li .item-content .item-inner .item-input{
    position: relative;
}
.page .bind_phone .list-block ul li .item-content .item-inner .item-input input{
    font-size: .75rem;
    width: 80%;
}
.page .bind_phone .list-block ul li .item-content .item-inner .item-input input::-webkit-input-placeholder {
    color: rgba(0, 0, 0, 0.1);
}
.page .bind_phone .list-block ul li .item-content .item-inner .item-input .code_btn{
    font-size: .6rem;
    font-weight: 300;
    color: rgba(0, 0, 0, 0.5);
    box-shadow: 0 0 2px 0 rgba(0, 0, 0, 0.5);
    border: none;
    text-align: center;
    padding: 0.1rem 0.15rem;
    position: absolute;
    right: .2rem;
    top: .5rem;
}
.page .bind_phone .bind_btn{
    display: flex;
    display: -webkit-flex;
    height: 1.2rem;
    line-height: 1.2rem;
    margin-top: .4rem;
}
.page .bind_phone .bind_btn div{
    flex: 1;
    -webkit-flex: 1;
    text-align: center;
    font-size: .8rem;
}
.page .bind_phone .bind_btn .bind_cancle{
    border-right: 1px solid rgba(0, 0, 0, 0.2);
}
.page .bind_phone .bind_btn .bind_comfir{
    color: rgb(222,63,55);
}

.cover{
    display:none;
    position:absolute;
    top:0;
    left:0;
    width:100%;
    height:100%;
    background-color:black;
    z-index:1001;
    -moz-opacity:0.3;
    opacity:.30;
    filter:alpha(opacity=30);
}

.content .wx_box{
    width: 95%;
    margin: .5rem auto 0;
    height: 2rem;
    background: rgb(250,250,250);
    display: flex;
    display: -webkit-flex;
}
.content .wx_box .wx_pic{
    flex: 1;
    -webkit-flex: 1;
}
.content .wx_box .wx_pic img{
    width: 2rem;
    height: 2rem;
    border-radius: 100%;
    -webkit-border-radius: 100%;
}
.content .wx_box .wx_text{
    flex: 5;
    -webkit-flex: 5;
    line-height: 2rem;
    height: 2rem;
    color: rgba(0,0,0,0.8);
    font-size: .8rem;
    font-weight: 300;
    overflow: hidden;
    text-overflow: ellipsis;
    word-break: break-all;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 1;
    display: -webkit-box !important;
}

/*商品详情*/
.content .content_block .tabs .goods_box{
    margin: .5rem auto;
    width: 100%;
    height: auto;
    background: #fff;
}
.content .content_block .tabs .goods_box .goods_state img{
    width: 3rem;
    height: 3rem;
    position: absolute;
    top: 3.5rem;
    right: 1rem;
    z-index: 5;
}

/*轮播图*/
.content .content_block .tabs .goods_box .swiper-container .swiper-slide img{
    width: 100%;
    height: 10rem;
}
.swiper-container-horizontal>.swiper-pagination-bullets, .swiper-pagination-custom, .swiper-pagination-fraction {
    bottom: .5rem;
    left: 0;
    width: 100%;
}
.content .content_block .swiper-container .swiper-slide{
    text-align: center;
    /* Center slide text vertically */
    display: -webkit-flex;
    display: flex;
    -webkit-box-pack: center;
    -ms-flex-pack: center;
    -webkit-justify-content: center;
    justify-content: center;
    -webkit-box-align: center;
    -ms-flex-align: center;
    -webkit-align-items: center;
    align-items: center;
}

/*商品标题简介*/
.content .content_block .goods_box .goods_brief{
    width: 90%;
    margin: .2rem auto;
    padding-bottom: .1rem;
}
.content .content_block .goods_box .goods_brief .brief_title{
    font-size: .85rem;
    font-weight: 300;
    display: -webkit-box !important;
    overflow: hidden;
    text-overflow: ellipsis;
    word-break: break-all;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
    height: 2.2rem;
    line-height: 1.1rem;
    margin: 0;
}
.end_time{
    font-size: .7rem;
    font-weight: 300;
    padding-left: 45px;
}
.content .content_block .goods_box .goods_brief .brief_price{
    margin-top: -.8rem;
    font-size: .95rem;
    font-weight: 300;
    color: rgb(222,63,55);
}
.content .content_block .goods_box .goods_brief .brief_price .price_one{
    border: 0.5px solid rgb(222,63,55);
    width: 3.2rem;
    font-weight: 300;
    margin-right: .2rem;
    font-size: .65rem;
}
.content .content_block .goods_box .goods_brief .brief_price .price_two{
    font-size: .7rem;
    font-weight: 300;
    color: rgba(0,0,0,0.4);
    margin-left: .2rem;
}
.content .content_block .goods_box .goods_brief .brief_price .price_three{
    color: rgb(35,35,35);
    font-size: .7rem;
}
.content .content_block .goods_box .goods_brief .brief_price .price_three .bonus{
    color: rgb(222,63,55);
}


/*商品详情中tab标签切换按钮*/
.content .content_block .tabs .buttons-tab .button{
    flex: 1;
    -webkit-flex: 1;
    text-align: center;
}
.content .content_block .tabs .buttons-tab .button.active{
    border-bottom: 2px solid rgb(222,63,55);
    color: rgba(0, 0, 0, 0.8) !important;
}

.content .content_block .goods_spec{
    background: #fff;
}
.content .content_block .goods_spec .spec_box{
    width: 100%;
    margin: 0 auto;
    font-size: .75rem;
    font-weight: 300;
    color: rgb(0,0,0);
    padding: .8rem .8rem;
    -webkit-box-shadow: 0 1px 1px -1px rgba(0,0,0,0.5);
}
.content .content_block .goods_spec .spec_box .spec_item{
    height: auto;
}
.content .content_block .goods_spec .spec_box .spec_item .spec_list{
    padding: .2rem .4rem;
    margin-left: .2rem;
    background: rgba(0,0,0,0.04);
    font-size: .65rem;
    font-weight: 300;
    border-radius: 6px;
    -webkit-border-radius: 6px;
    margin-top: .4rem;
    display: inline-flex;
}
.content .content_block .goods_spec .spec_box .spec_item .none{
    color: rgba(0,0,0,0.2);
    pointer-events: none;
}
.content .content_block .goods_spec .spec_box .spec_item .active{
    background: rgb(222,63,55);
    color: rgb(255,255,255);
}
.content .content_block .goods_spec .spec_num{
    display: flex;
    display: -webkit-flex;
    justify-content: space-between;
    -webkit-justify-content: space-between;
}
.content .content_block .goods_spec .spec_num .num_title{
    flex: 2;
    -webkit-flex: 2;
    width: 100%;
    text-align: left;
}
.content .content_block .goods_spec .spec_num .num_box{
    flex: 1;
    -webkit-flex: 1;
    box-shadow:0 0 1px 0 rgba(0, 0, 0, 0.5);
    border-radius: 10px;
    -webkit-border-radius: 10px;
    display: flex;
    display: -webkit-flex;
    flex-wrap: nowrap;
    -webkit-flex-wrap: nowrap;
}
.content .content_block .goods_spec .spec_num .num_box .subtract{
    flex: 1;
    -webkit-flex: 1;
    text-align: center;
    border-right:1px solid rgba(0, 0, 0, 0.5);
}
.content .content_block .goods_spec .spec_num .num_box .num_handle{
    width: 40%;
    text-align: center;
    border: none;
}
.content .content_block .goods_spec .spec_num .num_box .add{
    flex: 1;
    -webkit-flex: 1;
    text-align: center;
    border-left:1px solid rgba(0, 0, 0, 0.5);
}

.content .content_block .goods_text{
    margin: .5rem auto;
    width: 100%;
}
.content .content_block .goods_text .active{
    border-bottom: 2px solid rgb(222,63,55);
}

.content .content_block .tabs .text_goods, .page .content_block .goods_detail .tabs .parameter, .page .content_block .goods_detail .tabs .to_know{
    margin: -1rem auto 0;
    font-size: .85rem;
    width: 100%;
    font-weight: 300;
    color: rgba(0,0,0,0.6);
}
.content .content_block .goods_detail .tabs .tab .content-block{
    padding: 0  0.1rem;
    margin-top: 1rem;
}
.content .content_block .goods_detail .tabs .text_goods, .page .content_block .goods_detail .tabs .parameter, .page .content_block .goods_detail .tabs .to_know{
    width: 100%;
    overflow-x: hidden !important;
    margin: -1rem auto 0;
    font-size: .85rem;
    font-weight: 300;
    color: rgba(0,0,0,0.6);
}
.content .content_block .tab .pic_goods img{
    height: 10rem;
    width: 100%;
}
.content .content_block .tabs .parameter{
    width: 100%;
    margin: -1rem auto 0;
 }
.content .content_block .tabs .parameter tr{
    display: flex;
    display: -webkit-flex;
}
.content .content_block .tabs .parameter tr td:first-of-type{
    flex: 1;
    -webkit-flex: 1;
    color: rgba(0,0,0,0.6);
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}
.content .content_block .tabs .parameter tr td:last-of-type{
    flex: 2;
    -webkit-flex: 2;
    overflow: hidden;
    text-overflow: ellipsis;
}
.content .content_block .tabs .to_know .know_box h5{
    color: rgb(0,0,0);
    margin: .2rem;
}
.content .content_block .tabs .to_know .know_box p{
    margin: 0;
}
.content .content_block .tabs .to_know .know_else{
    display: flex;
    display: -webkit-flex;
    flex-wrap: wrap;
    -webkit-flex-wrap: wrap;
    justify-content: space-between;
    -webkit-justify-content: space-between;
}
.content .to_know .know_else img{
    margin-top: .4rem;
}
.content .content_block .tabs .tab .content-block{
    margin: .5rem 0 0;
    padding: 0 0.1rem;
}
@media(max-width: 320px){
    .content .wx_box .wx_text{
        font-size: .75rem;
    }
    .content .content_block .tabs .buttons-tab .button{
        font-size: .7rem;
    }
    nav .go_change{
        font-size: .75rem;
    }
    .content .content_block .goods_spec .spec_box{
        font-size: .7rem;
    }
    .content .content_block .goods_box .goods_brief .brief_title{
        font-size: .75rem;
        height: 2rem;
        line-height: 1rem;
    }
    .content .content_block .goods_box .goods_brief .brief_price .price_one{
        font-size: .6rem;
    }
    .content .content_block .goods_box .goods_brief .brief_price {
        font-size: .85rem;
    }
    .content .content_block .goods_spec .spec_box .spec_item .spec_list{
        max-width: 14rem;
    }
    .page .bind_phone{
        margin-top: -38%;
        height: 8.5rem;
    }
    .page .bind_phone .bind_title{
        font-size: .75rem;
        height: 2rem;
        line-height: 2rem;
    }
    .page .bind_phone .bind_btn div{
        font-size: .75rem;
    }
}
