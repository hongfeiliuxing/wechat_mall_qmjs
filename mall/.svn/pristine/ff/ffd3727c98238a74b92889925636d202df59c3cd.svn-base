.page {
  background: #f8f8f8;
  font-family: "PingFangSC-Regular" !important;
}
.page .bar-tab {
  height: auto;
  background: #fafafa;
}
.page .bar-tab .tab-item .icon {
  height: 1.4rem;
  margin: .2rem 0;
}
.page .bar-tab .tab-item .icon img {
  width: 1.4rem;
  height: 1.4rem;
}
.page .bar-tab .tab-item .tab-label {
  padding-bottom: .4rem;
  font-size: .65rem;
  font-weight: 300;
  color: rgba(0, 0, 0, 0.8);
}
.page .content .up_box {
  width: 100%;
  background: #d64343;
}
.page .content .up_box .people_infor {
  width: 50%;
  margin: 0 auto;
  padding: 1.2rem 0;
  text-align: center;
}
.page .content .up_box .people_infor .people_pic {
  width: 6.5rem;
  height: 6.5rem;
  margin: 0 auto;
}
.page .content .up_box .people_infor .people_pic img {
  width: 6.5rem;
  height: 6.5rem;
  border-radius: 100%;
  -webkit-border-radius: 100%;
  border: 4px solid #c9c9c9;
}
.page .content .up_box .people_infor .people_rel {
  font-size: 0.6rem;
  font-weight: 300;
  color: #ffffff;
  width: 25%;
  margin: 0 auto;
}
.page .content .up_box .people_infor .people_rel p {
  box-shadow: 0 0 2px 0 #ffffff;
}
.page .content .up_box .people_infor .people_name {
  font-size: 1rem;
  font-weight: 300;
  text-align: center;
  margin-top: -0.4rem;
  color: #ffffff;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  height: 1.2rem;
  line-height: 1.2rem;
}
.page .content .up_box .people_infor .people_code p {
  margin-top: 0;
  font-size: .65rem;
  font-weight: 300;
  color: #ffffff;
}
.page .content .up_box .edit {
  position: absolute;
  top: .8rem;
  right: .8rem;
}
.page .content .up_box .edit a img {
  width: 1.2rem;
  height: 1.2rem;
}
.page .content .mid_box{
  background: rgb(255,255,255);
}
.page .content .mid_box .income {
  width: 90%;
  margin: 0 auto;
  height: 3.8rem;
}
.page .content .mid_box .income a {
  width: 100%;
  color: #000000;
  display: inline-flex;
  display: -webkit-inline-flex;
  justify-content: space-between;
  -webkit-justify-content: space-between;
}
.page .content .mid_box .income a .income_l {
  flex: 5;
  -webkit-flex: 5;
}
.page .content .mid_box .income a .income_l p {
  font-size: .65rem;
  font-weight: 300;
  color: rgba(0, 0, 0, 0.6);
}
.page .content .mid_box .income a .income_l p span {
  color: #de3f37;
  font-size: 0.7rem;
  font-weight: 300;
}
.page .content .mid_box .income a .income_l p span span {
  font-size: 1.3rem;
  font-weight: 300;
}
.page .content .mid_box .income a .income_r {
  flex: 1;
  -webkit-flex: 1;
  text-align: right;
  height: 4rem;
  line-height: 4rem;
}
.page .content .mid_box .income_detail {
  padding-top: .5rem;
  padding-bottom: .5rem;
  margin: 0;
  display: flex;
  display: -webkit-flex;
  border-top: 1px solid rgba(0,0,0,0.1);
}
.page .content .mid_box .income_detail .income_num {
  flex: 1;
  -webkit-flex: 1;
  text-align: center;
}
.page .content .mid_box .income_detail .income_num span {
  color: rgba(0, 0, 0, 0.6);
  font-size: 0.7rem;
  font-weight: 300;
}
.page .content .mid_box .income_detail .income_num .span_f {
  color: #000000;
  font-size: 1rem;
  font-weight: 300;
}
.page .content .mid_box .income_detail .income_num .span_f span{
  font-size: 1rem;
  font-weight: 300;
}
.page .content .null {
  height: .5rem;
  background: rgba(0, 0, 0, 0.05);
}
.page .content .down_box {
  margin-bottom: 2rem;
  display: flex;
  display: -webkit-flex;
  flex-wrap: wrap;
  -webkit-flex-wrap: wrap;
  background: rgb(255,255,255);
}
.page .content .down_box div {
  width: 33%;
  height: 5rem;
  text-align: center;
  border-bottom: 1px solid rgba(0,0,0,0.1);
  border-right: 1px solid rgba(0,0,0,0.1);
}
.page .content .down_box div a {
  width: 50%;
  margin: auto;
}
.page .content .down_box div a img {
  width: 1.6rem;
  height: 1.6rem;
  margin-top: 1rem;
}
.page .content .down_box div a span {
  color: rgba(0, 0, 0, 0.6);
  font-size: .75rem;
  font-weight: 300;
}
.page .content .down_box div a .badge {
  background: #de3f37;
  color: #ffffff;
  font-size: 0.6rem;
  font-weight: 300;
  position: relative;
  top: -3.6rem;
  right: -1rem;
}
.page .content .down_box div:nth-child(3n) {
  width: 34%;
  border-right: none;
}
