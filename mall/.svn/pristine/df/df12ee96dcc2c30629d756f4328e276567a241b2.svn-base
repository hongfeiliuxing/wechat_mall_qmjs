.page {
  background: #fafafa;
  font-family: "PingFangSC-Regular" !important;
}
.page .content .up_box {
  width: 100%;
  background: #d64343;
  height: 6.8rem;
  display: flex;
  display: -webkit-flex;
  justify-content: space-between;
  -webkit-justify-content: space-between;
}
.page .content .up_box .box_l {
  flex: 3;
  -webkit-flex: 3;
  position: relative;
}
.page .content .up_box .box_l .img_box {
  position: absolute;
  top: 1.1rem;
  right: 0;
  height: 4.6rem;
  width: 4.6rem;
  background: #d7d7d7;
  border-radius: 100%;
  -webkit-border-radius: 100%;
}
.page .content .up_box .box_l .img_box img {
  width: 4.3rem;
  height: 4.3rem;
  border-radius: 100%;
  -webkit-border-radius: 100%;
  margin-top: .15rem;
  margin-left: .15rem;
}
.page .content .up_box .box_m {
  flex: 4;
  -webkit-flex: 4;
}
.page .content .up_box .box_m .ship_box {
  margin-left: 1rem;
  height: 3.6rem;
  line-height: 1.8rem;
  margin-top: 1.6rem;
  width: 6rem;
}
.page .content .up_box .box_m .ship_box span:first-child {
  box-shadow: 0 0 2px 0 #ffffff;
  font-size: .6rem;
  padding: 0 0.2rem;
  color: rgba(255, 255, 255, 0.8);
}
.page .content .up_box .box_m .ship_box span:last-child {
  color: #ffffff;
  font-size: .8rem;
  font-weight: 300;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  display: block;
}
.page .content .up_box .box_r {
  flex: 3;
  -webkit-flex: 3;
}
.page .content .up_box .box_r img {
  width: 4.2rem;
}
.page .content .content-block {
  margin: .5rem 0 0;
  padding: 0;
}
.page .content .content-block .con-block .list_container {
  margin: 0;
  padding: 0;
  list-style: none;
}
.page .content .content-block .con-block .list_container li {
  padding: .5rem .3rem .3rem;
  border-bottom: 1px solid rgba(0, 0, 0, 0.1);
  display: flex;
  display: -webkit-flex;
}
.page .content .content-block .con-block .list_container li .li_l {
  flex: 2;
  -webkit-flex: 2;
}
.page .content .content-block .con-block .list_container li .li_l img {
  width: 6.5rem;
  height: 6.5rem;
}
.page .content .content-block .con-block .list_container li .li_r {
  flex: 3;
  -webkit-flex: 3;
}
.page .content .content-block .con-block .list_container li .li_r .up_con {
  overflow: hidden;
  display: -webkit-box !important;
  text-overflow: ellipsis;
  word-break: break-all;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  font-size: .75rem;
  font-weight: 300;
  height: 1.85rem;
  line-height: .95rem;
  color: #4e4e4e;
}
.page .content .content-block .con-block .list_container li .li_r .mid_con {
  margin-top: 1.5rem;
  display: flex;
  display: -webkit-flex;
}
.page .content .content-block .con-block .list_container li .li_r .mid_con .pri_l {
  flex: 2;
  -webkit-flex: 2;
  font-size: .65rem;
}
.page .content .content-block .con-block .list_container li .li_r .mid_con .pri_l p {
  margin: 0;
  color: #fa5b52;
}
.page .content .content-block .con-block .list_container li .li_r .mid_con .pri_l p .pri_one {
  font-size: 1rem;
  font-weight: 600;
  display: inline-block;
}
.page .content .content-block .con-block .list_container li .li_r .mid_con .pri_l p s {
  color: #999999;
  display: inline-block;
  margin-left: .8rem;
  font-size: .7rem;
}
.page .content .content-block .con-block .list_container li .li_r .mid_con .pri_r {
  flex: 1;
  -webkit-flex: 1;
}
