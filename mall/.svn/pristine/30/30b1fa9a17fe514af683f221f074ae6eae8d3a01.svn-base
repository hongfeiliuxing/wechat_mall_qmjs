.page{
    background: rgb(250,250,250);
}
.page .bar-header-secondary {
    top: 0;
}
.bar-header-secondary~.content {
    top: 2.5rem;
}
.bar-nav~.content {
    top: 0.1rem;
}
.content .buttons-tab{
    background: rgb(250,250,250);
    width: 90%;
    margin: 0 auto;
}

.content .buttons-tab .button{
    font-size: .8rem;
    font-weight: 300;
    color: rgb(0,0,0);
}
.content .buttons-tab .button.active{
    border-bottom-color: rgb(222,63,55);
    color: rgb(222,63,55);
}
.content .content-block{
    margin: -.4rem 0 0;
    padding: 0;
    background: rgb(255,255,255);
}

.page .content .content-block .con-block .list_container{
    list-style: none;
    padding: 0;
}
.page .content .content-block .con-block .list_container li{
    padding: .5rem .3rem .3rem 0;
    border-bottom: 1px solid rgba(0, 0, 0, 0.1);
}

.page .content .content-block .con-block .list_container li .item-link{
    display: inline-flex;
    display: -webkit-inline-flex;
    padding: 0;
    line-height: 0;
    width: 100%;
}
.page .content .con-block .list_container li .item-link .selling_pic{
    flex: 2;
    -webkit-flex: 2;
    text-align: center;
    position: relative;
}
.page .content .con-block .list_container li .item-link .selling_pic img{
    width: 6.5rem;
    height: 6.5rem;
}

.page .content .list_container li .item-link .selling_pic .selling_icon{
    position: absolute;
    top: 0;
    left: .1rem;
    width: 1.2rem;
    height: 1.2rem;
}
.page .content .list_container li .item-link .selling_pic .selling_icon img{
    width: 1.2rem;
    height: 1.2rem;
}
.page .content .list_container li .item-link .selling_text{
    /*padding-left: .2rem;*/
    flex: 3;
    -webkit-flex: 3;
    text-align: left;
}
.page .content .list_container li .item-link .selling_text .selling_title{
    display: -webkit-box !important;
    overflow: hidden;
    text-overflow: ellipsis;
    word-break: break-all;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
    font-size: .85rem;
    font-weight: 300;
    height: 2.2rem;
    line-height: 1.1rem;
    width: 100%;
    color: rgb(78,78,78);
}
.page .content .list_container li .item-link .selling_text .selling_price{
    color: rgb(222,63,55);
    display: flex;
    display: -webkit-flex;
    margin-top: 1.5rem;
    height: 1.5rem;
    line-height: 1.5rem;
}
.page .content .list_container li .item-link .selling_text .selling_price .pri_l{
	font-size: .65rem;
    flex: 2;
    -webkit-flex: 2;
}
.page .content .list_container li .item-link .selling_text .selling_price .pri_l p{
	margin: 0;
}
.page .content .list_container li .item-link .selling_text .selling_price .pri_l .pri_one{
	font-size: 1rem;
    font-weight: 600;
    display: inline-block;
}
.page .content .list_container li .item-link .selling_text .selling_price .pri_l s{
	display: inline-block;
    margin-left: .8rem;
    color: rgb(229,229,229);
    font-size: .7rem;
}
.page .content .list_container li .item-link .selling_text .selling_price .pri_r{
	flex: 1;
    -webkit-flex: 1;
    font-size: .65rem;
    padding-top: .2rem;
}
.page .content .list_container li .item-link .selling_text .go_buy{
	display: flex;
	display: -webkit-flex;
}
.page .content .list_container li .item-link .selling_text .go_buy .sur_l{
	flex: 2;
    -webkit-flex: 2;
    margin-top: .5rem;
}
.page .content .list_container li .item-link .selling_text .go_buy .sur_l .surplus{
	background: rgba(222,63,55,0.47);
    border-radius: 8px;
    -webkit-border-radius: 8px;
    width: 4.5rem;
    height: .8rem;
    line-height: .8rem;
    display: flex;
    display: -webkit-flex;
    position: relative;
    margin: 0;
}
.page .content .list_container li .item-link .selling_text .go_buy .sur_l .surplus .span_f{
	background: rgb(222,63,55);
    height: .8rem;
    line-height: .8rem;
    border-radius: 8px;
    -webkit-border-radius: 8px;
}
.page .content .list_container li .item-link .selling_text .go_buy .sur_l .surplus .span_s{
	text-align: center;
    position: absolute;
    color: rgb(255,255,255);
    width: 9rem;
    left: -4.5rem;
    top: -.8rem;
    font-size: 1rem;
    font-weight: 300;
    height: 1.6rem;
    line-height: 1.6rem;
    transform: scale(0.5);
    -webkit-transform: scale(0.5);
    -webkit-transform-origin: 100% 100%;
    transform-origin: 100% 100%;
}
.page .content .list_container li .item-link .selling_text .go_buy .sur_r{
	flex: 1;
    -webkit-flex: 1;
    background: rgb(222,63,55);
    color: rgb(255,255,255);
    font-size: .65rem;
    height: 1.2rem;
    line-height: 1.2rem;
    border-radius: 8px;
    -webkit-border-radius: 8px;
    margin-top: .1rem;
    text-align: center;
}
.page .content .list_container li .item-link .selling_text .go_buy .sur_r img{
	height: .5rem;
    display: inline-block;
    margin-left: .1rem;
    margin-top: -.1rem;
    vertical-align: middle;
}
@media (max-width:320px) {
    .page .content .content-block .list_container li{
        padding: .6rem .4rem .2rem .4rem;
    }
    .page .content .list_container li .item-link .selling_pic img{
        width: 3.5rem;
        height: 3.5rem;
    }
    .page .content .list_container li .item-link .selling_pic .selling_icon{
        margin-top: -4.2rem;
    }
    .page .content .list_container li .item-link .selling_text .selling_title{
        font-size: .75rem;
        height: 1.9rem;
        line-height: .9rem;
        margin-top: -0.15rem;
    }
    .page .content .list_container li .item-link .selling_text .selling_price .price_detail .price_one{
        height: 1rem;
        margin-top: .7rem;
    }
    .page .content .list_container li .item-link .selling_text .selling_price .price_detail .deadline{
        margin-top: 0;
    }
}