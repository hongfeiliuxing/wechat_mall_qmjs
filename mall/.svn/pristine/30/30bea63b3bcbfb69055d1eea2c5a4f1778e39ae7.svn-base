.page {
  background: #f8f8f8;
  font-family: PingFangSC-Regular !important;
}
.page .content .list-block {
  margin: 0;
}
.page .content .list-block ul li .item-content .item-inner {
  padding-right: 0;
}
.page .content .list-block ul li .item-content .item-inner .item-title.label {
  width: 20%;
}
.page .content .list-block ul li .item-content .item-inner .item-input {
  position: relative;
}
.page .content .list-block ul li .item-content .item-inner .item-input input {
  font-size: 0.85rem;
}
.page .content .list-block ul li .item-content .item-inner .item-input input::-webkit-input-placeholder {
  color: rgba(0, 0, 0, 0.1);
}
.page .content .list-block ul li .item-content .item-inner .item-input .code_btn {
  position: absolute;
  right: 1rem;
  top: .5rem;
}
.page .content .list-block ul li .item-content .item-inner .item-input .code_btn img {
  width: 1.1rem;
  height: 1.1rem;
}
.page .content .btn_box {
  background: #de3f37;
  width: 90%;
  margin: 1rem auto 0;
  text-align: center;
  color: #ffffff;
  height: 2.5rem;
  line-height: 2.5rem;
  border-radius: 5px;
  -webkit-border-radius: 5px;
  font-weight: 300;
}
.modal.modal-in {
  width: 90%;
  margin-left: -45%;
}
.modal.modal-in .modal-inner {
  padding: 0.75rem 0.5rem 0.3rem;
}
.modal.modal-in .modal-inner .big_box {
  width: 100%;
}
.modal.modal-in .modal-inner .big_box .up_box {
  display: flex;
  display: -webkit-flex;
  justify-content: space-between;
  -webkit-justify-content: space-between;
  padding-top: .5rem;
  padding-bottom: .2rem;
  border-bottom: 1px solid rgba(0, 0, 0, 0.1);
  font-size: .85rem;
  font-weight: 300;
}
.modal.modal-in .modal-inner .big_box .up_box .up_l {
  flex: 4;
  -webkit-flex: 4;
  text-align: left;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.modal.modal-in .modal-inner .big_box .up_box .up_r {
  flex: 1;
  -webkit-flex: 1;
  text-align: right;
  color: #de3f37;
}
.modal.modal-in .modal-inner .big_box .mid_box {
  padding: .5rem 0 0;
  display: flex;
  display: -webkit-flex;
}
.modal.modal-in .modal-inner .big_box .mid_box .mid_l {
  flex: 3;
  -webkit-flex: 3;
}
.modal.modal-in .modal-inner .big_box .mid_box .mid_l img {
  width: 3.5rem;
  height: 3.5rem;
}
.modal.modal-in .modal-inner .big_box .mid_box .mid_c {
  margin-left: .3rem;
  flex: 7;
  -webkit-flex: 7;
  text-align: left;
}
.modal.modal-in .modal-inner .big_box .mid_box .mid_c .text_name {
  height: 2.6rem;
  line-height: 1.3rem;
  overflow: hidden;
  display: -webkit-box !important;
  text-overflow: ellipsis;
  word-break: break-all;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  font-size: .8rem;
  font-weight: 300;
  color: #000000;
  margin-top: -0.1rem;
}
.modal.modal-in .modal-inner .big_box .mid_box .mid_c .text_data {
  font-size: .7rem;
  font-weight: 300;
  color: #444444;
  opacity: 72;
}
.modal.modal-in .modal-inner .big_box .mid_box .mid_r {
  flex: 3;
  -webkit-flex: 3;
  text-align: right;
  font-size: 0.75rem;
  font-weight: 300;
}
.modal.modal-in .modal-inner .big_box .mid_box .mid_r .text_pri {
  margin-bottom: 1.3rem;
}
.modal.modal-in .modal-buttons .modal-button {
  color: #f9ae39;
}
@media screen and (max-width: 320px) {
  .page .content .list-block ul li .item-content .item-inner .item-title.label {
    font-size: 0.75rem;
  }
  .page .content .list-block ul li .item-content .item-inner .item-input input {
    font-size: 0.75rem;
  }
  .page .content .btn_box {
    font-size: 0.75rem;
    height: 2.2rem;
    line-height: 2.2rem;
  }
  .modal.modal-in .modal-inner .big_box .up_box {
    font-size: 0.75rem;
    padding-bottom: .3rem;
  }
  .modal.modal-in .modal-inner .big_box .mid_box .mid_l img {
    width: 3.2rem;
    height: 3.2rem;
  }
  .modal.modal-in .modal-inner .big_box .mid_box .mid_c .text_name {
    font-size: 0.75rem;
  }
  .modal.modal-in .modal-inner .big_box .mid_box .mid_c .text_data {
    font-size: .6rem;
  }
  .modal.modal-in .modal-buttons .modal-button {
    font-size: .75rem;
  }
}
@media screen and (min-width: 320px) and (max-width: 374px) {
  .page .content .list-block ul li .item-content .item-inner .item-title.label {
    font-size: 0.8rem;
  }
  .page .content .list-block ul li .item-content .item-inner .item-input input {
    font-size: 0.8rem;
  }
  .page .content .btn_box {
    font-size: 0.8rem;
    height: 2.4rem;
    line-height: 2.4rem;
  }
  .modal.modal-in .modal-inner .big_box .up_box {
    font-size: 0.8rem;
    padding-bottom: .3rem;
  }
  .modal.modal-in .modal-inner .big_box .mid_box .mid_l img {
    width: 3.2rem;
    height: 3.2rem;
  }
  .modal.modal-in .modal-inner .big_box .mid_box .mid_c .text_name {
    font-size: 0.8rem;
  }
  .modal.modal-in .modal-inner .big_box .mid_box .mid_c .text_data {
    font-size: .65rem;
  }
  .modal.modal-in .modal-inner .big_box .mid_box .mid_r .text_pri {
    margin-bottom: 1.2rem;
  }
  .modal.modal-in .modal-buttons .modal-button {
    font-size: 0.8rem;
  }
}
