.page{
    background: rgb(250,250,250);
}
.page .bar-header-secondary {
    top: 0;
}
.bar-header-secondary~.content {
    top: 2.5rem;
}
.bar-nav~.content {
    top: 0.1rem;
}
.content .buttons-tab{
    background: rgb(250,250,250);
    width: 90%;
    margin: 0 auto;
}

.content .buttons-tab .button{
    font-size: .8rem;
    font-weight: 300;
    color: rgb(0,0,0);
}
.content .buttons-tab .button.active{
    border-bottom-color: rgb(222,63,55);
    color: rgb(222,63,55);
}
.content .content-block{
    margin: 0;
    padding: 0;
}
.page .content .content-block .list_container{
    list-style: none;
    margin: 0 0 0 -2.2rem;
}
.page .content .content-block .list_container li{
    /*width: 100%;*/
    padding: 0.5rem 0.3rem 0.3rem 0;
    /*border-bottom: 1px solid rgba(0, 0, 0, 0.1);*/
}
.page .content .content-block .list_container li .selling_item{
    display: inline-flex;
    display: -webkit-inline-flex;
    padding: 0;
    line-height: 0;
    width: 100%;
}
.page .content .list_container li .selling_item .selling_pic{
    flex: 2;
    -webkit-flex: 2;
}
.page .content .list_container li .selling_item .selling_pic img{
    width: 6.5rem;
    height: 6.5rem;
}

.page .content .list_container li .selling_item .selling_pic .selling_icon{
    margin-top: -4.8rem;
    margin-left: -0.2rem;
    width: 1.2rem;
    height: 1.2rem;
}
.page .content .list_container li .selling_item .selling_pic .selling_icon img{
    width: 1.2rem;
    height: 1.2rem;
}
.page .content .list_container li .selling_item .selling_text{
    /*padding-left: .2rem;*/
    flex: 3;
    -webkit-flex: 3;
    text-align: left;
    /*color: #000000;
    display: flex;
    display: -webkit-flex;
    flex-direction: column;
    -webkit-flex-direction: column;*/
}
.page .content .list_container li .selling_item .selling_text .selling_title{
    overflow: hidden;
    display: -webkit-box !important;
    text-overflow: ellipsis;
    word-break: break-all;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
    font-size: .75rem;
    font-weight: 300;
    height: 1.85rem;
    line-height: .95rem;
    color: rgb(78,78,78);
}
.page .content .list_container li .selling_item .selling_text .selling_price{
    width: 100%;
}
.page .content .list_container li .selling_item .selling_text .selling_price .price_detail{
    width: 100%;
    color: #de3f37;
    display: flex;
    display: -webkit-flex;
    justify-content: space-between;
    -webkit-justify-content: space-between;
}
.page .content .list_container li .selling_item .selling_text .selling_price .price_detail .price_one{
    margin: .9rem 0 0;
    flex: 1;
    -webkit-flex: 1;
    font-size: .8rem;
    font-weight: 300;
}
.page .content .list_container li .selling_item .selling_text .selling_price .price_detail .price_one s{
    color: rgba(0, 0, 0, 0.4);
    font-size: .65rem;
    margin-left: .2rem;
    font-weight: 300;
}
.page .content .list_container li .selling_item .selling_text .selling_price .price_detail .deadline {
    flex: 1;
    -webkit-flex: 1;
    margin: .2rem 0 0;
    text-align: right;
    color: #000000;
    font-size: .6rem;
    font-weight: 300;
}
.page .content .list_container li .selling_item .selling_text .selling_price .price_detail .deadline .bonus {
    background: #de3f37;
    border-radius: 5px;
    -webkit-border-radius: 5px;
    text-align: center;
    color: #ffffff;
    display: inline-block;
    width: 4.5rem;
}
.page .content .list_container li .selling_item .selling_text .selling_price .price_detail .deadline .end_time {
    color: #de3f37;
    margin-left: -0.1rem;
    font-size: .65rem;
    font-weight: 300;
}
.page .content .list_container li .item_n .selling_text .selling_price .price_detail .deadline{
    margin-top: 1.1rem;
}
@media (max-width:320px) {
    .page .content .content-block .list_container li{
        padding: .6rem .4rem .2rem .4rem;
    }
    .page .content .list_container li .selling_item .selling_pic img{
        width: 3.5rem;
        height: 3.5rem;
    }
    .page .content .list_container li .selling_item .selling_pic .selling_icon{
        margin-top: -4.2rem;
    }
    .page .content .list_container li .selling_item .selling_text .selling_title{
        font-size: .75rem;
        height: 1.9rem;
        line-height: .9rem;
        margin-top: -0.15rem;
    }
    .page .content .list_container li .selling_item .selling_text .selling_price .price_detail .price_one{
        height: 1rem;
        margin-top: .7rem;
    }
    .page .content .list_container li .selling_item .selling_text .selling_price .price_detail .deadline{
        margin-top: 0;
    }
}