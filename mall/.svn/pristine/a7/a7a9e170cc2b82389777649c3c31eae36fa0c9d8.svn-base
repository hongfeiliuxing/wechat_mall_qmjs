.page{
  background: rgb(247,247,247);
  font-family: "PingFangSC-Regular" !important;
  overflow-x:hidden !important;
  overflow-y:auto;
}
.page.content{
  overflow-x: hidden !important;
}
.page .content .sale_box {
  margin-top: -.6rem;
  margin-bottom: 0;
  width: 100%;
  background: rgb(255,255,255);
  text-align: left !important;
  padding: 0 0.1rem;
  overflow-x: hidden !important;
}
.page .content .sale_box .detail_intro{
  background: rgb(255,255,255);
  overflow-x: hidden !important;
}
.page .content .sale_list {
  margin-top: .5rem;
  background: #ffffff;
  overflow-x: hidden !important;
}
.page .content .sale_list .up_list {
  width: 38%;
  margin: 0 auto;
  padding: 1rem 0 .5rem;
  display: flex;
  display: -webkit-flex;
}
.page .content .sale_list .up_list .hd_ico {
  flex: 1;
  -webkit-flex: 1;
}
.page .content .sale_list .up_list .hd_ico img {
  width: 2rem;
  height: 2rem;
}
.page .content .sale_list .up_list .hd_text {
  flex: 2;
  -webkit-flex: 2;
}
.page .content .sale_list .up_list .hd_text p {
  font-size: .8rem;
  font-weight: 300;
  line-height: 1.1rem;
  color: rgba(0, 0, 0, 0.8);
  margin: .1rem 0 0;
}
.page .content .sale_list .up_list .hd_text p span {
  font-size: .5rem;
  font-weight: 300;
  margin-top: -0.2rem;
  display: -webkit-box;
}
.page .content .sale_list .con_list {
  width: 100%;
  border-top: 1px solid rgba(0, 0, 0, 0.1);
}
.page .content .sale_list .con_list .list_container {
  width: 100%;
  list-style: none;
  margin: 0;
  padding: 0;
}
.page .content .sale_list .con_list .list_container li {
  padding: .8rem .4rem .4rem .6rem;
  border-bottom: 1px solid rgba(0, 0, 0, 0.1);
}
.page .content .sale_list .con_list .list_container li .selling_item {
  width: 100%;
  display: -webkit-inline-flex;
  display: inline-flex;
  justify-content: space-between;
  -webkit-justify-content: space-between;
}
.page .content .sale_list .con_list .list_container li .selling_item .selling_pic {
  flex: 1;
  -webkit-flex: 1;
}
.page .content .sale_list .con_list .list_container li .selling_item .selling_pic img {
  width: 4rem;
  height: 4rem;
}
.page .content .sale_list .con_list .list_container li .selling_item .selling_pic .selling_icon {
  margin-top: -4.8rem;
  margin-left: -0.2rem;
  width: 1.2rem;
  height: 1.2rem;
}
.page .content .sale_list .con_list .list_container li .selling_item .selling_pic .selling_icon img {
  width: 1.2rem;
  height: 1.2rem;
}
.page .content .sale_list .con_list .list_container li .selling_item .selling_text {
  padding-left: .2rem;
  flex: 3;
  -webkit-flex: 3;
  color: #000000;
  display: flex;
  display: -webkit-flex;
  flex-direction: column;
  -webkit-flex-direction: column;
}
.page .content .sale_list .con_list .list_container li .selling_item .selling_text .selling_title {
  display: -webkit-box !important;
  overflow: hidden;
  text-overflow: ellipsis;
  word-break: break-all;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  font-size: .85rem;
  font-weight: 300;
  height: 2.2rem;
  line-height: 1.1rem;
  width: 100%;
}
.page .content .sale_list .con_list .list_container li .selling_item .selling_text .selling_price {
  width: 100%;
}
.page .content .sale_list .con_list .list_container li .selling_item .selling_text .selling_price .price_detail {
  width: 100%;
  color: #de3f37;
  display: flex;
  display: -webkit-flex;
  justify-content: space-between;
  -webkit-justify-content: space-between;
}
.page .content .sale_list .con_list .list_container li .selling_item .selling_text .selling_price .price_detail .price_one {
  margin: .9rem 0 0;
  flex: 3;
  -webkit-flex: 3;
  font-size: .85rem;
  font-weight: 300;
}
.page .content .sale_list .con_list .list_container li .selling_item .selling_text .selling_price .price_detail .price_one span{
  font-size: .85rem;
}
.page .content .sale_list .con_list .list_container li .selling_item .selling_text .selling_price .price_detail .price_one s {
  color: rgba(0, 0, 0, 0.4);
  font-size: .65rem;
  font-weight: 300;
  margin-left: .2rem;
}
.page .content .sale_list .con_list .list_container li .selling_item .selling_text .selling_price .price_detail .deadline {
  flex: 2;
  -webkit-flex: 2;
  margin: .2rem 0 0;
  text-align: right;
  color: #000000;
  font-size: .6rem;
  font-weight: 300;
}
.page .content .sale_list .con_list .list_container li .selling_item .selling_text .selling_price .price_detail .deadline .bonus {
  background: #de3f37;
  border-radius: 5px;
  text-align: center;
  color: #ffffff;
  width: 5rem;
  display: block;
}
.page .content .sale_list .con_list .list_container li .selling_item .selling_text .selling_price .price_detail .deadline .end_time {
  color: #de3f37;
  margin-left: -0.1rem;
}
@media(max-width: 320px){
  .page .content .sale_list .con_list .list_container li{
    padding: .6rem .4rem .2rem .5rem;
  }
  .page .content .sale_list .con_list .list_container li .selling_item .selling_pic img{
    width: 3.5rem;
    height: 3.5rem;
  }
  .page .content .sale_list .con_list .list_container li .selling_item .selling_pic .selling_icon{
    margin-top: -4.2rem;
  }
  .page .content .sale_list .con_list .list_container li .selling_item .selling_text .selling_title{
    font-size: .75rem;
    height: 1.9rem;
    line-height: .9rem;
    margin-top: -0.15rem;
  }
  .page .content .sale_list .con_list .list_container li .selling_item .selling_text .selling_price .price_detail .price_one{
    height: 1rem;
    margin-top: .7rem;
  }
  .page .content .sale_list .con_list .list_container li .selling_item .selling_text .selling_price .price_detail .deadline{
    margin-top: 0;
  }
}
